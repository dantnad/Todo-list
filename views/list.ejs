<%- include('header') %>
    <div class="box">
      <h1 id="heading"><%=listTitle%></h1>
    </div>

    <div class="box">
      <% listOfTasks.forEach(function(element){ %>
      <div class="item">
        <input type="checkbox" />
        <p><%=element%></p>
      </div>
      <% })%>
      <form class="item" action="/" method="post">
        <input autocomplete="off" type="text" name="task" placeholder="What's the task?" />
        <button type="submit" name="list" value="<%= listTitle %>">+</button>
      </form>
    </div>
<%- include('footer') %>